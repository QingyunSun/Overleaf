	Date-Modified = {2014-11-12 22:09:12 +0000},
	Journal = {Control Systems, IEEE},
	Number = {3},
	Pages = {52--65},
	Publisher = {IEEE},
	Title = {Receding horizon control},
	Volume = {31},
	Year = {2011},
